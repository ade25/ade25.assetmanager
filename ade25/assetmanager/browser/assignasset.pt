<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="ck.sitecontent">
<body>

<metal:main fill-slot="main">
  <metal:content-core define-macro="main">

    <div class="app-page app-page-manager">
        <div class="pm-toolbar">
          <div class="pm-navbar">
            <div class="pm-navbar-item pm-navbar-back">
              <a tal:attributes="href context/absolute_url">
                <i class="ion ion-ios7-arrow-thin-left pm-icon"></i>
                Overview
              </a>
            </div>
            <div class="pm-navbar-item pm-navbar-title">
              <span i18n:translate="">Manage Assets</span>
            </div>
          </div>
          <div class="pm-locator">Assign Asset</div>
        </div>
        <div class="app-page-body app-asset-manager">
            <div class="app-asset-listing">
            </div>
        </div>
    </div>
  </metal:content-core>
</metal:main>
</body>
</html>